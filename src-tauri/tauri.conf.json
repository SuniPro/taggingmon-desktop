{
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
	"productName": "Taggingmon",
	"version": "0.1.0",
	"identifier": "com.taggingmon",
	"build": {
		"beforeDevCommand": "pnpm dev",
		"beforeBuildCommand": "pnpm build",
		"devUrl": "http://localhost:5173",
		"frontendDist": "../build/client"
	},
	"app": {
		"windows": [
			{
				"title": "Taggingmon",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
